{
  "name": "Halon",
  "context": "..",
  "dockerFile": "./Dockerfile",
  "forwardPorts": [
    25
  ],
  "overrideCommand": false,
  "extensions": [
    "Halon.vscode-halon",
    "Halon.hsl-linter",
    "dbaeumer.vscode-eslint"
  ],
  "mounts": [
    "source=${localWorkspaceFolder}/sampleWorkspace/dist,target=/etc/halon,type=bind,consistency=cached"
  ]
}